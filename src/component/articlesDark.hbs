{{#each data}}
<div>
	<div class="articles_card">
		<div>
			<img src="{{photo}}" alt="Photo" class="articles_img" id="{{id}}">
			<div class="articles_isNew">
				{{#if isNew}}
				<div class="articles_logo">NEW</div>
				{{/if}}
			</div>
		</div>
		<strong class="articles_name">{{name}}</strong>
		<p class="articles_description">{{description}}</p>
		<a href="#" class="articles_link">Read All</a>
	</div>
</div>
{{/each}}