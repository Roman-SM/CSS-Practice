<header>
	{{> headerDark data=header}}
</header>

<main class="main">
	{{#with hero}}
	<div class="hero">
		<h1 class="hero_title">{{title}}</h1>
		<p class="hero_description">{{description}}</p>
		<a href="{{button.url}}" class="hero_button">{{button.text}}</a>
	</div>
	{{/with}}
</main>

<div class="about">
	{{#with about}}
	<div class="about_list">
		<h2 class="about_heading">{{heading}}</h2>
		<p class="about_info">{{info}}</p>
	</div>
	{{/with}}
</div>

<div class="articles">
	<div class="articles_nav">
		<strong class="articles_title">Our Articles</strong>
		<a href="#" class="articles_botton">View All</a>
	</div>
	<div class="articles_list">
		{{> articlesDark data=articles}}
	</div>
</div>

<footer class="footer">
	{{> footerDark data=footer}}
</footer>